<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<groups>
		<run>
			<include name="Error Handling"/>
		</run>
	</groups>
  <test thread-count="5" name="StandaloneTest">
    <classes>
      <class name="Selenium.tests.StandaloneTest"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="ErrorValidationsTest">
    <classes>
      <class name="Selenium.tests.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
